static int marsExploration(string s) {
            int count = 0;
            for(int i =0; i<s.Length; i++)
            {
               if(i % 3 == 0 && s[i] != 'S') count++;
               else if (i % 3 == 1 && s[i] != 'O') count++;
               else if (i % 3 == 2 && s[i] != 'S') count++;
            }
            return count; 

    }
